{
  "expo": {
    "name": "TicketPrinter",
    "slug": "TicketPrinter",
    "version": "1.0.0",
    "orientation": "portrait",
    "plugins": [
      "@config-plugins/react-native-ble-plx",
      [
        "expo-build-properties",
        {
          "android": {
            "minSdkVersion": 23
          }
        }
      ]
    ],
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.koffeekat.TicketPrinter"
    },
    "android": {
      "package": "com.koffeekat.TicketPrinter"
    },
    "extra": {
      "eas": {
        "projectId": "5d9cc568-213e-4dda-aa38-109556eaf2cb"
      }
    }
  }
}
